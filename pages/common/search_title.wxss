.common_title_layout{
  width: 100%;
  height: 64px;
  display: flex;
  float: top;
  position: fixed;
  top: 0px;
  left: 0px;
  z-index: 1000000;
}

.back_text{
  background: none;
  margin-top: 20px;
  line-height: 44px;
  font-size: 15px;
  text-align: center;
  color: white;
}

.back{
  width: 24px;
  height: 24px;
  margin-left: 10px;
  margin-top: 30px;
}

.title{
  color: white;
  width: 100%;
  text-align: center;
  font-size: 19px;
  margin-top: 20px;
  line-height: 44px;
  margin-left: 10px;
  margin-right: 80px;
}

.search_title_input{
  border-radius: 15px;
  padding-left: 15px;
  padding-right: 15px;
  margin-top: 27px;
  background: white;
  height: 30px;
  margin-right: 10px;
  margin-left: 10px;
  width: 100%;
}

